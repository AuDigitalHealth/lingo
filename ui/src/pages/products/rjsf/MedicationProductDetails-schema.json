{
  "type": "object",
  "$defs": {
    "Ingredient": {
      "type": "object",
      "properties": {
        "activeAndPreciseIngredients": {
          "type": "object",
          "properties": {
            "activeIngredient": {
              "$ref": "#/$defs/SnowstormConceptMini"
            },
            "preciseIngredient": {
              "$ref": "#/$defs/SnowstormConceptMini"
            }
          },
          "required": ["genericForm"],
          "title": "Generic and Specific Forms"
        },
        "basisOfStrengthSubstance": {
          "$ref": "#/$defs/SnowstormConceptMini"
        },
        "concentrationStrength": {
          "$ref": "#/$defs/Quantity"
        },
        "totalQuantity": {
          "$ref": "#/$defs/Quantity"
        }
      }
    },
    "Map(String,Object)": {
      "type": "object"
    },
    "Quantity": {
      "type": "object",
      "properties": {
        "unit": {
          "$ref": "#/$defs/SnowstormConceptMini"
        },
        "value": {
          "type": "number"
        }
      }
    },
    "SnowstormConceptMini": {
      "type": "object",
      "properties": {
        "conceptId": {
          "type": "string"

        }

      }

    },
    "ExternalIdentifier": {
      "type": "object",
      "properties": {
        "identifierScheme": {
          "type": "string",
          "title": "Identifier Scheme",
          "default": "https://www.tga.gov.au/artg"
        },
        "identifierValue": {
          "type": "string",
          "title": "Identifier Value"
        }
      }
    }
  },
  "properties": {
    "productName": {
      "$ref": "#/$defs/SnowstormConceptMini",
      "title": "Brand Name",
      "minProperties": 1
    },
    "containerType": {
      "$ref": "#/$defs/SnowstormConceptMini",
      "title": "Container Type",
      "minProperties": 1
    },
    "externalIdentifiers": {
      "type": "array",
      "title": "External Identifiers",
      "items": {
        "$ref": "#/$defs/ExternalIdentifier"
      }
    },
    "containedProducts": {
      "type": "array",
      "title": "Contained Products",
      "items": {
        "type": "object",
        "properties": {
          "productDetails": {
            "type": "object",
            "title": "Product Details",
            "properties": {
              "productName": {
                "$ref": "#/$defs/SnowstormConceptMini",
                "title": "Product Name",
                "minProperties": 1
              },
              "activeIngredients": {
                "type": "array",
                "title": "Active Ingredients",
                "items": {
                  "$ref": "#/$defs/Ingredient"
                }
              },
              "genericAndSpecificForms": {
                "type": "object",
                "properties": {
                  "genericForm": {
                    "$ref": "#/$defs/SnowstormConceptMini",
                    "title": "Generic Dose Form"
                  },
                  "specificForm": {
                    "$ref": "#/$defs/SnowstormConceptMini",
                    "title": "Specific Dose Form"
                  }
                },
                "required": ["genericForm"],
                "title": "Generic and Specific Forms"
              },
              "quantity": {
                "$ref": "#/$defs/Quantity"
              },
              "containerType": {
                "$ref": "#/$defs/SnowstormConceptMini",
                "title": "Container Type"
              },
              "deviceType": {
                "$ref": "#/$defs/SnowstormConceptMini",
                "title": "Device Type"
              },
              "packSize": {
                "$ref": "#/$defs/Quantity"
              },
              "otherIdentifyingInformation": {
                "type": "string",
                "title": "Other Identifying Information"
              }
            },
            "required": ["productName", "activeIngredients", "packSize"],
            "mutuallyExclusive": ["containerType", "deviceType"]
          }
        }
      }
    }
  },
  "required": ["productName", "containerType"],
  "additionalProperties": false
}
